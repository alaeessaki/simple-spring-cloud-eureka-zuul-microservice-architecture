package com.alae.web;

import org.springframework.beans.factory.annotation.Value;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class CompayRestService {
	
	@Value("${xParam}")
	private int xParam;
	
	@Value("${yParam}")
	private int yParam;
	
	@Value("${me}")
	private String me;
	
	
	
	
}
